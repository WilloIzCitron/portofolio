{
    "version": 2,
    "installCommand": "yarn install",
    "buildCommand": "yarn build",
    "builds": [{ "src": "api/index.js", "use": "@vercel/node", "config": { "includeFiles": ["api/**"] } }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.js"
        }
    ]
}