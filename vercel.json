{
    "version": 2,
    "builds": [
        {
            "src": "/src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/src/(.*)",
            "dest": "/src"
        },
        {
            "src": "/public/(.*)",
            "dest": "/public"
        }
    ]
}